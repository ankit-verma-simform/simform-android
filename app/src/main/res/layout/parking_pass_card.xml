<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/guest_secure_checkout_card_bg"
    android:elevation="10dp"
    android:paddingBottom="20dp"
    android:theme="@style/Theme.AndroidDemoProject.GuestSecureCheckout">

    <TextView
        android:id="@+id/tvConfirmationNumberLabel"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginTop="14dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/confirmation_no"
        android:textColor="@color/white_gray_waterloo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextSizeCheck,TextContrastCheck" />

    <TextView
        android:id="@+id/tvConfirmationNumber"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/confirmation_no_value"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvConfirmationNumberLabel"
        tools:ignore="TextSizeCheck" />

    <ImageView
        android:id="@+id/imgParkingReceipt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/parking_receipt_image_bg"
        android:clipToOutline="true"
        android:importantForAccessibility="no"
        android:scaleType="centerCrop"
        android:src="@drawable/bitmap"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvConfirmationNumber"
        tools:ignore="UnusedAttribute" />

    <TextView
        android:id="@+id/tvParkingNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@color/light_green_swans_down"
        android:fontFamily="@font/poppins_medium"
        android:padding="6dp"
        android:text="@string/parking_number"
        android:textColor="@color/green_atoll"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/imgParkingReceipt"
        app:layout_constraintTop_toBottomOf="@id/imgParkingReceipt" />

    <TextView
        android:id="@+id/tvAddress1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="16dp"
        android:ellipsize="marquee"
        android:fadingEdge="horizontal"
        android:fontFamily="@font/poppins_medium"
        android:marqueeRepeatLimit="marquee_forever"
        android:scrollHorizontally="true"
        android:singleLine="true"
        android:text="@string/address_1"
        android:textSize="15sp"
        app:layout_constraintEnd_toStartOf="@id/imgQRCode"
        app:layout_constraintStart_toEndOf="@id/tvParkingNo"
        app:layout_constraintTop_toTopOf="@id/tvParkingNo" />

    <TextView
        android:id="@+id/tvAddress2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="16dp"
        android:ellipsize="marquee"
        android:fadingEdge="horizontal"
        android:fontFamily="@font/poppins_regular"
        android:marqueeRepeatLimit="marquee_forever"
        android:scrollHorizontally="true"
        android:singleLine="true"
        android:text="@string/address_2"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@id/imgQRCode"
        app:layout_constraintStart_toStartOf="@id/tvParkingNo"
        app:layout_constraintTop_toBottomOf="@id/tvParkingNo" />

    <ImageView
        android:id="@+id/imgQRCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/qr_code"
        android:src="@drawable/qr_code"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imgParkingReceipt" />

    <TextView
        android:id="@+id/tvTapToZoom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/tap_to_zoom"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@id/imgQRCode"
        app:layout_constraintStart_toStartOf="@id/imgQRCode"
        app:layout_constraintTop_toBottomOf="@id/imgQRCode"
        tools:ignore="TextContrastCheck" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnDirection"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:background="@drawable/parking_card_button_direction_bg"
        android:fontFamily="@font/poppins_medium"
        android:gravity="center"
        android:paddingHorizontal="13dp"
        android:paddingTop="5dp"
        android:text="@string/direction"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@id/tvParkingNo"
        app:layout_constraintTop_toBottomOf="@id/tvAddress2"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/tvEnterAfter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/enter_after"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/btnDirection"
        app:layout_constraintTop_toBottomOf="@id/btnDirection"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/tvEnterAfterDateTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/enter_after_date_time"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvEnterAfter"
        app:layout_constraintStart_toEndOf="@id/tvEnterAfter"
        app:layout_constraintTop_toTopOf="@id/tvEnterAfter" />

    <ImageButton
        android:id="@+id/btnEditEnterAfter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="16dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/edit_date_and_time_for_enter_after"
        android:paddingBottom="5dp"
        android:src="@drawable/ic_guest_secure_checkout_pencil"
        app:layout_constraintBottom_toBottomOf="@id/tvEnterAfterDateTime"
        app:layout_constraintStart_toEndOf="@id/tvEnterAfterDateTime"
        app:layout_constraintTop_toTopOf="@id/tvEnterAfterDateTime"
        app:tint="?colorPrimary"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tvExitBefore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/exit_before"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/btnDirection"
        app:layout_constraintTop_toBottomOf="@id/tvEnterAfter"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/tvExitBeforeDateTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/exit_before_date_time"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvExitBefore"
        app:layout_constraintStart_toStartOf="@id/tvEnterAfterDateTime"
        app:layout_constraintTop_toTopOf="@id/tvExitBefore" />

    <ImageButton
        android:id="@+id/btnEditExitBefore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="16dp"
        android:background="@android:color/transparent"
        android:contentDescription="@string/edit_date_and_time_for_exit_before"
        android:paddingBottom="5dp"
        android:src="@drawable/ic_guest_secure_checkout_pencil"
        app:layout_constraintBottom_toBottomOf="@id/tvExitBeforeDateTime"
        app:layout_constraintStart_toEndOf="@id/tvExitBeforeDateTime"
        app:layout_constraintTop_toTopOf="@id/tvExitBeforeDateTime"
        app:tint="?colorPrimary"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tvVehicleMake"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/vehicle"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/btnDirection"
        app:layout_constraintTop_toBottomOf="@id/tvExitBefore"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/tvVehicleAndMake"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/vehicle_make_and_model_data"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvVehicleMake"
        app:layout_constraintStart_toStartOf="@id/tvExitBeforeDateTime"
        app:layout_constraintTop_toTopOf="@id/tvVehicleMake" />

    <TextView
        android:id="@+id/tvTotalHoursLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/total_hrs"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/btnDirection"
        app:layout_constraintTop_toBottomOf="@id/tvVehicleMake"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/tvTotalHours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/total_hours_value"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvTotalHoursLabel"
        app:layout_constraintStart_toStartOf="@id/tvVehicleAndMake"
        app:layout_constraintTop_toTopOf="@id/tvTotalHoursLabel" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="16dp"
        android:background="@color/gray_alto"
        app:layout_constraintTop_toBottomOf="@id/tvTotalHours" />

    <TextView
        android:id="@+id/tvPaymentDateLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/payment_date"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/btnDirection"
        app:layout_constraintTop_toBottomOf="@id/divider"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/tvPaymentDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/payment_date_value"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvPaymentDateLabel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvPaymentDateLabel"
        app:layout_constraintTop_toTopOf="@id/tvPaymentDateLabel" />

    <TextView
        android:id="@+id/tvPaymentTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/payment_time"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@id/tvPaymentDate"
        app:layout_constraintTop_toBottomOf="@id/tvPaymentDate" />

    <TextView
        android:id="@+id/tvPaymentMethodLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/payment_method"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/tvPaymentDateLabel"
        app:layout_constraintTop_toBottomOf="@id/tvPaymentTime"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/tvPaymentMethod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/apple_pay"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvPaymentMethodLabel"
        app:layout_constraintStart_toStartOf="@id/tvPaymentTime"
        app:layout_constraintTop_toTopOf="@id/tvPaymentMethodLabel" />

    <TextView
        android:id="@+id/tvOriginalPriceLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/original_price"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/tvPaymentMethodLabel"
        app:layout_constraintTop_toBottomOf="@id/tvPaymentMethodLabel"
        tools:ignore="TextContrastCheck,VisualLintBounds" />

    <TextView
        android:id="@+id/tvOriginalPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/original_price_value"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvOriginalPriceLabel"
        app:layout_constraintStart_toStartOf="@id/tvPaymentMethod"
        app:layout_constraintTop_toTopOf="@id/tvOriginalPriceLabel"
        tools:ignore="VisualLintBounds" />

    <TextView
        android:id="@+id/tvServiceFeeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/service_fee"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/tvOriginalPriceLabel"
        app:layout_constraintTop_toBottomOf="@id/tvOriginalPriceLabel"
        tools:ignore="TextContrastCheck,VisualLintBounds" />

    <TextView
        android:id="@+id/tvServiceFee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/service_fee_value"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvServiceFeeLabel"
        app:layout_constraintStart_toStartOf="@id/tvOriginalPrice"
        app:layout_constraintTop_toTopOf="@id/tvServiceFeeLabel"
        tools:ignore="VisualLintBounds" />

    <TextView
        android:id="@+id/tvTotalLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/total"
        android:textColor="@color/white_gray_waterloo"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@id/tvServiceFeeLabel"
        app:layout_constraintTop_toBottomOf="@id/tvServiceFeeLabel"
        tools:ignore="TextContrastCheck,VisualLintBounds" />

    <TextView
        android:id="@+id/tvTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/total_value"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@id/tvTotalLabel"
        app:layout_constraintStart_toStartOf="@id/tvServiceFee"
        app:layout_constraintTop_toTopOf="@id/tvTotalLabel"
        tools:ignore="VisualLintBounds" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnBuySpaceAgain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/poppins_medium"
        android:text="@string/buy_this_space_again"
        android:textColor="?colorPrimary"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@id/tvTotalLabel"
        app:layout_constraintTop_toBottomOf="@id/tvTotalLabel"
        tools:ignore="VisualLintBounds,TouchTargetSizeCheck" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnShare"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/parking_receipt_outlined_button_bg"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/share"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@id/btnDownload"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnBuySpaceAgain"
        tools:ignore="VisualLintButtonSize,VisualLintBounds" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnDownload"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:background="@drawable/parking_receipt_outlined_button_bg"
        android:fontFamily="@font/poppins_semibold"
        android:text="@string/download"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/btnShare"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/btnShare"
        app:layout_constraintTop_toTopOf="@id/btnShare"
        tools:ignore="VisualLintBounds,VisualLintButtonSize" />
</androidx.constraintlayout.widget.ConstraintLayout>